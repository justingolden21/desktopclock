{
	"name": "desktop-clock",
	"version": "1.0.0",
	"description": "A simple, resizable and customizable clock for any device",
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"preview": "svelte-kit preview",
		"lint": "prettier --ignore-path .gitignore  --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"generate-assets": "pwa-asset-generator src/img/icons/icon.svg src/img/icons --manifest src/localized/en/manifest.webmanifest --index src/app.html --favicon --mstile --icon-only --opaque true"
	},
	"keywords": [
		"clock",
		"desktop clock",
		"clock app",
		"analog clock",
		"digital clock",
		"customizable clock",
		"free clock",
		"simple clock",
		"date and time",
		"clock with date",
		"fullscreen clock",
		"bedtime clock",
		"resizable clock",
		"clock for windows",
		"clock for mac",
		"clock for chrome",
		"clock for android",
		"clock for ios"
	],
	"author": "Justin Golden",
	"license": "MIT",
	"devDependencies": {
		"@sveltejs/adapter-netlify": "^1.0.0-next.27",
		"@sveltejs/kit": "next",
		"autoprefixer": "^10.3.1",
		"cssnano": "^5.0.6",
		"eslint": "^7.22.0",
		"eslint-config-airbnb-base": "^14.2.1",
		"eslint-config-prettier": "^8.1.0",
		"eslint-plugin-import": "^2.23.4",
		"eslint-plugin-svelte3": "^3.2.0",
		"postcss": "^8.3.5",
		"postcss-import": "^14.0.2",
		"postcss-load-config": "^3.1.0",
		"prettier": "~2.2.1",
		"prettier-plugin-svelte": "^2.2.0",
		"pwa-asset-generator": "^4.1.1",
		"svelte": "^3.34.0",
		"svelte-preprocess": "^4.7.4",
		"tailwindcss": "^2.2.4"
	},
	"type": "module"
}
