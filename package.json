{
	"name": "desktop-clock",
	"version": "1.0.5",
	"description": "A simple, resizable customizable, installable clock for any device!",
	"scripts": {
		"dev": "svelte-kit dev --open",
		"build": "svelte-kit build",
		"preview": "svelte-kit preview",
		"buildpreview": "svelte-kit build && svelte-kit preview",
		"lint": "prettier --ignore-path .gitignore --ignore-path .prettierignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --ignore-path .prettierignore --write --plugin-search-dir=. .",
		"generate-assets": "pwa-asset-generator static/img/icons/icon.svg static/img/icons --favicon --mstile --icon-only --opaque true"
	},
	"author": "Justin Golden",
	"private": "true",
	"devDependencies": {
		"@sveltejs/adapter-netlify": "1.0.0-next.49",
		"@sveltejs/kit": "1.0.0-next.291",
		"autoprefixer": "^10.4.7",
		"cssnano": "^5.1.8",
		"eslint": "^7.32.0",
		"eslint-config-airbnb-base": "^14.2.1",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-svelte3": "^3.4.1",
		"postcss": "^8.4.14",
		"postcss-import": "^14.1.0",
		"postcss-load-config": "^3.1.4",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"pwa-asset-generator": "^4.3.9",
		"screenfull": "^5.2.0",
		"svelte": "^3.48.0",
		"svelte-preprocess": "^4.10.6",
		"tailwindcss": "^3.0.24"
	},
	"type": "module",
	"dependencies": {
		"dayjs": "^1.11.2",
		"lodash.merge": "^4.6.2"
	}
}
